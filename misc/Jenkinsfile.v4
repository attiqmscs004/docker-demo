pipeline {
    agent any
    stages {
        stage('AppSec') {
            steps {
                bat 'exit 0'
            }
        }
        stage('Approval') {
            steps {
                catchError(buildResult: 'SUCCESS', stageResult: 'FAILURE') {
                    bat "exit 1"
                }
            }
        }
        stage('NewStage') {
            steps {
                bat 'exit 0'
            }
        }
    }
}
